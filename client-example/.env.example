# Foxglove API Configuration
# Copy this file to .env and fill in your actual values

# Required: Your Foxglove API token
# Get this from: https://studio.foxglove.dev -> Settings -> API Keys
FOXGLOVE_API_TOKEN=fox_sk_your_api_token_here

# Optional: Logging level
# Options: error, warn, info, debug, trace
RUST_LOG=info

# Optional: API Environment (currently only production supported)
FOXGLOVE_ENV=production